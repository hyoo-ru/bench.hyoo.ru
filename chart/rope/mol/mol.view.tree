$mol_app_bench_chart_rope_mol $mol_view
	sub /
		<= Chart $mol_chart
			graphs <= graphs /$mol_plot_graph
				<= Vert $mol_plot_ruler_vert
					title \Val
				<= Hor $mol_plot_ruler_hor
					title \Iter
	Graph!id $mol_plot_group
		title <= graph_title!id \
		series_y <= series!id /
		graphs /
			<= Line!id $mol_plot_line
			<= Dots!id $mol_plot_dot
